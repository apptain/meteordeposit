@import '_variables';
@import '_mixins';

body
	font-family $f-primary

header, logowrap
	padding 1em
	border-bottom solid 4px #000

ul.blog-post-list
	li
		list-style-type none
		line-height step(1, 2, $phi, em)
	h3
		font-family $f-header
		font-weight 700
		font-size 22px
		line-height 28px
		margin 10px 0px 0px 0px






